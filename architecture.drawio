        <mxCell id="7OVz4w7n9kzEPWy-4BBC-13" value="wettr.xyz is registered on Namecheap domain registry. In Namecheap the DNS is pointed to the DNS servers provided by Route53." style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1159.5" y="21.5" width="449" height="36.5" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-14" value="Hosted zone wettr.xyz in Route53. The hosted zone holds the routing configuration and verifies the SSL certificates." style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1158.5" y="68" width="450" height="40" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-15" value="&lt;span&gt;CloudFront distribution serves the fronted hosted in the S3 bucket. CloudFront replicates the content of the edge locations, so the website loads fast regardless of the region of the bucket where the website is hosted. Additionally, CloudFront does the SSL termination for the frontend.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1159.5" y="118" width="450" height="90" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-16" value="&lt;span&gt;S3 bucket used to host the frontend. It contains the necessary JS/HTML/CSS, images, etc. which are needed for the frontend.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1158.5" y="208" width="450" height="40" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-17" value="&lt;span&gt;API gateway is configured to handle the calls coming through the address api.wettr.xyz. The calls are dispatched to different Lambda functions depending on the path in the URL.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1158.5" y="258" width="449" height="60" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-18" value="&lt;span&gt;A lambda function which return the locations supported by the application.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1159.5" y="318" width="450" height="40" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-19" value="&lt;span&gt;Lambda function to get the weather forecast for a given location. The forecast data is read form a DynamoDB table, transformed and returned in JSON format.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
          <mxGeometry x="1158.5" y="370" width="449" height="58" as="geometry" />
        </mxCell>
        <mxCell id="7OVz4w7n9kzEPWy-4BBC-20" value="&lt;span&gt;Lambda function to get the summary of the current weather conditions for a given location. The data holds the textual description of the weather conditions and the link to the audio file which is synthetized from the textual description.&lt;/span&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;" parent="1" vertex="1">
